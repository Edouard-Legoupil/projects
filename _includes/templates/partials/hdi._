<div id='chart-hdi'>
  <h3>Human Development Index</h3>
  <span class='chart-legend' style='font-size:0.8em; font-weight:normal; color:#999; margin-bottom:5px;'>
    <div class='thick-bar clearfix' style='float:left; width:90px;'>
      <span style='display:block; float:left; width:30px; height:15px; background:#c8dde4; margin-right:5px'></span>
      Country
    </div>
    <div class='thin-bar' style='float:left;'>
      <span style='display:block; float:left; width:30px; height:4px; background:#c8dde4; margin-right:5px; margin-top:5px;'></span>
      Global Avg
    </div>
  </span>
  <div class='hdic'>
    <div class='caption'>
      <div>HDI</div>
      <div>Health</div>
      <div>Education</div>
      <div>Income</div>
    </div>
    <div class='data'> </div>
    <div class='details'>
      <div class='clearfix rankchange'>
        <div class='ranking'></div>
        <div class='change'></div>
      </div>
      <div id='sparkline'></div>
      <div id='xlabel' style='font-size:9px;'>
        <span class='beginyear'>1980</span>
        <span class='endyear' style='float:right;'>2011</span>
      </div>
    </div>
  </div>
</div>
